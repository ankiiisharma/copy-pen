{
  "manifest_version": 2,
  "name": "My Notes Extension",
  "version": "1.0",
  "description": "A simple notes extension with copy functionality.",
  "permissions": ["clipboardWrite"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon1.png",
      "48": "icons/icon1.png",
      "128": "icons/icon1.png"
    }
  },
  "icons": {
    "16": "icons/icon1.png",
    "48": "icons/icon1.png",
    "128": "icons/icon1.png"
  },
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  }
}
